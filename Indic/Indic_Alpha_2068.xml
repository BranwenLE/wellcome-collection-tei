                    <msContents>
                        
                        <summary>The <title>Gaṅgālaharī</title> composed by <persName role="aut">Jagannātha Paṇḍitarāja</persName>
                            in the <date>mid-seventeenth century</date>. See for the text <abbr>NCC</abbr>, vol. 5, pp. 214-216,
                            and for the author <abbr>NCC</abbr>, vol. 7, pp. 137-138. This version contains 59 verses instead of
                            the standard 53.</summary>
                        
                        <textLang mainLang="sa">Sanskrit</textLang>
                        
                        <msItem xml:id="Alpha_2068">
                            <title key="work_182001360">Gaṅgālaharī</title>
                            <author key="person_97929384">Jagannātha Paṇḍitarāja</author>
                            
                            <incipit> <locus>F. 1v</locus>
                                <!-- Transcript -->
                                śrīgaṇeśāya namaḥ ||
                                mahādānair dhyānair bahuvidhavitānair api cayan
                                na labhyam ghorābhiḥ suvima(pu)lataporājibhirapi ||
                                aciṃtyaṃ tad viṣṇoḥ padam akhilasādhāraṇatayā
                                dadānā kenāsitvam iha tulanīyā kathaya naḥ |
                            </incipit>
                            
                            <explicit> <locus>F. 13v</locus>
                                <!-- Transcript -->
                                umāṃ pīyūṣalaharīṃ
                                jagannāthena nirmitām ||
                                yaḥ paṭhet tasya sarvatra
                                jāyaṃter jaya(ṃte jaya)saṃpadaḥ ||59||
                                śrī 3 iti 2 (=iti śrī) paṃḍitarājaracitā gaṃgālaharī samāptā
                            </explicit>
                            
                        </msItem>
                        
